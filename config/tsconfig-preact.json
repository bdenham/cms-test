{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxImportSource": "preact",
    "jsxFragmentFactory": "Fragment",
    "paths": {
      "react": ["preact/compat"],
      "react-dom/test-utils": ["preact/test-utils"],
      "react-dom": ["preact/compat"],
      "react/jsx-runtime": ["preact/jsx-runtime"]
    }
  }
}
